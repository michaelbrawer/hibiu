    <li role="presentation">
    <% else %>
    <%= link_to 'Log In', login_path %>
    <%= link_to 'Sign Up', new_user_path %>
    <%end%>
  </li>
  <li role="presentation">
  
  </li>
  
  </ul>
  </nav>

    <%= yield %>
    <p>
  <% if current_user %>
    Welcome, <%= current_user.username %>
    &nbsp;&nbsp;|&nbsp;&nbsp;
    <%= link_to 'Log Out', session_path('dummy'), method: :delete %>
  <% else %>
    <%= link_to 'Log In', login_path %>
    &nbsp;&nbsp;|&nbsp;&nbsp;

  </head>

  <body>
<nav class="navbar navbar-header">

  <ul class="nav nav-pills">
  <li role="presentation" class="active">
  <% if current_user %>
    Welcome, <%= current_user.username %>
    
    <%= link_to 'Log Out', session_path('dummy'), method: :delete %>
